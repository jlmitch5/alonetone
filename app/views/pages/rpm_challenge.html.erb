<h2>February 2010</h2>
<%= cache(cache_digest(@albums_2010)+'v2') do %>
  <% @albums_2010.each do |album| %>
    <div class="album clearfix">
      <div class="cover">
        <%= link_to(image_tag('rpm/overlay.png'), user_playlist_path(album.user.login, album), class: 'overlay') %>
        <%= image_tag(playlist_cover_url(album, variant: :playlist_card) || 'default/no-cover-200.jpg', class: 'cover') %>
        <%= link_to awesome_truncate(album.title, 50), user_playlist_path(album.user.login, album), class: 'album_title' %><br/>
        <%= link_to album.user.name, user_home_path(album.user.login), class: 'alonetoner'%>
      </div>
    </div>
  <% end %>
  <div class="clearfix"> </div>
  <h2 style="margin-top:120px;">February 2009</h2>
  <% @albums_2009.each do |album| %>
    <div class="album clearfix">
      <div class="cover">
        <%= link_to(image_tag('rpm/overlay.png'), user_playlist_path(album.user.login, album), class: 'overlay') %>
        <%= image_tag(playlist_cover_url(album, variant: :playlist_card) || 'default/no-cover-200.jpg', class: 'cover') %>
        <%= link_to awesome_truncate(album.title,50), user_playlist_path(album.user.login, album), class: 'album_title' %><br/>
        <%= link_to album.user.name, user_home_path(album.user.login), class: 'alonetoner'%>
      </div>
    </div>
  <% end %>
<% end %>
<div class="clearfix"> </div>